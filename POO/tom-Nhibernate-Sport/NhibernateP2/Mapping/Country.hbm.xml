<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhibernateP2"
                   namespace="NhibernateP2">
  <class name="NhibernateP2.Country, NhibernateP2" table="tbl_country">
    
    <!--id-->
    <id name="Id" column="Id_Country" type="int">
      <generator class="identity"/>
    </id>

    <!--properties-->
    <property name="Code" column="Cd_Country" type="String"/>
    <property name="Name" column="Nm_Country" type="String"/>
    
  </class>
</hibernate-mapping>