<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhibernateP2"
                   namespace="NhibernateP2">
  <class name="NhibernateP2.Nationality, NhibernateP2" table="tbl_nationality">
    <id name="id_Athlete" column="Id_Athlete" type="int">
      <generator class="identity"/>
    </id>
    <bag name="Country" table="Athlete_x_Country">
      <key column="id_Athlete"/>
      <many-to-many class="Country" column="Id_Country"/>
    </bag>
    
  </class>
</hibernate-mapping>